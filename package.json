{
  "name": "necropolis-web",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "build": "yarn build:noprecache && workbox injectManifest workbox-config.js",
    "build:noprecache": "rm -rf dist && parcel build ./src/index.html && imagemin dist/* --out-dir=dist --plugin=mozjpeg --plugin=pngquant && cp src/assets/icons/android-icon-*.png dist/",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:cache": "yarn lint --cache",
    "typecheck": "tsc",
    "test": "BABEL_ENV=test jest",
    "e2e": "SERVE_APP=true cypress run",
    "e2e:open": "cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@parcel/packager-raw-url": "2.0.0-beta.2",
    "@parcel/transformer-webmanifest": "2.0.0-beta.2",
    "@types/animejs": "^3.1.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.14",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-modal": "^3.10.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "cypress": "^6.0.1",
    "esbuild": "^0.12.5",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "imagemin-cli": "^6.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "jest": "^27.0.3",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.0.1",
    "typescript": "^4.0.2",
    "workbox-cli": "^6.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.1.1",
    "@sentry/react": "^5.21.1",
    "animejs": "^3.2.1",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "polished": "^4.0.3",
    "random-seed": "^0.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.5",
    "typesafe-actions": "^5.1.0",
    "uuid": "^8.3.2",
    "workbox-precaching": "^6.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "cypress/*"
    ],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "mjs",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.m?[tj]sx?$": "esbuild-jest"
    },
    "modulePathIgnorePatterns": [
      "cache/*",
      "cypress/*",
      ".yarn"
    ]
  }
}
